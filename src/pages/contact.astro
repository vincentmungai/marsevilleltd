---
import "../styles/main.css";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
import Accordion from "../components/Accordion/Primary.astro";
import { Image } from 'astro:assets';
import customerCare from "../assets/images/customerCare.jpg";
import { allCompanyInformation } from "../lib/seoContent"

const seoContent = await allCompanyInformation();

const keywords = seoContent[0]?.seoKeywords?.join(", ") || "";

---
<!DOCTYPE html>

<html  data-wf-page="662cd8706408cf2a4b60ec52"
    data-wf-site="661caa5ae3dacb8bb23f4d00" lang="en" data-wf-locale="en">

<head>
    <meta charset="utf-8" />
    <title>Contact - {seoContent[0].companyName}</title>
    <meta
        content={seoContent[0].seoDescription}
        name="description"/>
    <meta content={`Contact - ${seoContent[0].companyEmail}`}  property="og:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="og:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="og:image" />
    <meta content={`Contact - ${seoContent[0].companyEmail}`} property="twitter:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="twitter:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
   
    <meta name="keywords" content={keywords} />
   
    
    
    

    <script
        type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="icon" type="image/png" />
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="apple-touch-icon" sizes="180x180" />
        
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-28TP2W8ST9"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-28TP2W8ST9');
</script>

</head>

<body>
<Navbar/>
    <section class="banner-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="banner-wrap">
                <div data-w-id="c0a18700-9b76-6b85-e93b-4ea49e796bd2" style="Opacity:1"
                    class="pricng-banner-title-wrap">
                    <h1 class="banner-title">Get in Touch</h1>
                    <div class="pricing-banner-content-wrap">
                        <p class="pricing-banner-content">Whether you have a question about services, prices, need a any
                            other details please contact us using the form and the other information on this page.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="contact-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="contact-whole-wrap">
                <div data-w-id="d1ddec97-f97d-6920-41b7-6bfc91609919" style="Opacity:1" class="contact-wrap">

                    <div class="contact-link-wrap">
                        <p class="contact-link-text-title">Address :</p>
                        <p class="contact-link-text">{seoContent[0].companyAddress}</p>
                        <p class="contact-link-text">Nairobi, Kenya</p>
                    </div>
                </div>
                <div data-w-id="6e86a264-9051-779b-8367-94992915480e" style="Opacity:1" class="contact-wrap">

                    <div class="contact-link-wrap">
                        <p class="contact-link-text-title">Contact :</p><a href={`tel:${seoContent[0].companyPhone}`}
                            class="contact-link-text contact-link">{seoContent[0].companyPhone}</a>
                    </div>
                </div>
                <div data-w-id="5d87910c-62ba-8bde-2f27-6ec1157a171b" style="Opacity:1" class="contact-wrap">
  
                    <div class="contact-link-wrap">
                        <p class="contact-link-text-title">Email :</p><a href={`mailto:${seoContent[0].companyEmail}`} 
                            class="contact-link-text contact-link">{seoContent[0].companyEmail}</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="contact-form-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="contact-form-wrap">
                <div data-w-id="53f0ab50-1685-4920-786f-5b867f021b77" style="Opacity:1" class="contact-image-wrap">
                    <Image src={customerCare}loading="eager" alt="contact section image" class="conatct-form-image" style="height: 400px; width: auto; object-fit: cover; object-position: right center;"/></div>
                <div data-w-id="168ab24b-9554-ef99-5f94-1ee98a2ad137" style="Opacity:1" class="contact-form-whole-wrap">
                    <div class="contact-form-block w-form">
                        <form action="https://api.web3forms.com/submit" id="wf-form-Contact-Email-Form" name="wf-form-Contact-Email-Form"
                            data-name="Contact Email Form" method="POST" class="contact-form"
                            data-wf-page-id="662cd8706408cf2a4b60ec52"
                            data-wf-element-id="168ab24b-9554-ef99-5f94-1ee98a2ad139">
                            <input type="hidden" name="access_key" value="fbfe5d8c-5f66-4c7f-9570-50d70e2abc13">
                            <div class="contact-form-field-whole-wrap">
                                <div class="contact-form-field-wrap"><label for="Contact-Name"
                                        class="contact-form-field-label">Your Name</label><input
                                        class="contact-text-field w-input" maxlength="256" name="Contact-Name"
                                        data-name="Contact Name" placeholder="John" type="text" id="Contact-Name"
                                        required="" /></div>
                                <div class="contact-form-field-wrap"><label for="Contact-Mail"
                                        class="contact-form-field-label">Your Email</label><input
                                        class="contact-text-field w-input" maxlength="256" name="Contact-Mail"
                                        data-name="Contact Mail" placeholder="John@mail.com" type="email"
                                        id="Contact-Mail" required="" /></div>
                            </div>
                            <div class="contact-form-field-whole-wrap">
                                <div class="contact-form-field-wrap"><label for="Contact-Phone"
                                        class="contact-form-field-label">Your Phone</label><input
                                        class="contact-text-field w-input" maxlength="256" name="Contact-Phone"
                                        data-name="Contact Phone" placeholder={seoContent[0].companyPhone} type="tel"
                                        id="Contact-Phone" required="" /></div>
                                <div class="contact-form-field-wrap"><label for="Contact-Subject"
                                        class="contact-form-field-label">Service</label><input
                                        class="contact-text-field w-input" maxlength="256" name="Contact-Subject"
                                        data-name="Contact Subject" placeholder="Delivery" type="text"
                                        id="Contact-Subject" /></div>
                            </div>
                            <div class="contact-form-field-wrap"><label for="Contact-Message"
                                    class="contact-form-field-label">Your Message Here</label><textarea
                                    id="Contact-Message" name="Contact-Message" maxlength="5000"
                                    data-name="Contact Message" placeholder="Message"
                                    class="contact-message-field w-input"></textarea></div><label
                                class="w-checkbox feedback-checkbox-field-wrap">
                                <div
                                    class="w-checkbox-input w-checkbox-input--inputType-custom feedback-form-check-box">
                                </div><input type="checkbox" name="By-clicking-on-this-button-I-agree-to-the-processi-2"
                                    id="By-clicking-on-this-button-I-agree-to-the-processi-2"
                                    data-name="By Clicking On This Button I Agree To The Processi 2"
                                    style="Opacity:1;position:absolute;z-index:-1" /><span
                                    class="feedback-checkbox-label w-form-label"
                                    for="By-clicking-on-this-button-I-agree-to-the-processi-2">By clicking on this
                                    button, I agree to the process</span>
                            </label>
                            <div class="contact-form-button-wrap"><input type="submit" data-wait="Please wait..."
                                    class="secondary-button w-button" value="Submit" style="color: white !important; /></div>
                        </form>
                        <div class="success-message w-form-done">
                            <div>Thank you! Your submission has been received!</div>
                        </div>
                        <div class="error-message w-form-fail">
                            <div>Oops! Something went wrong while submitting the form.</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <Accordion>
    <Footer/>
    
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=661caa5ae3dacb8bb23f4d00"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="../script/app.js"> </script>
</body>

</html>