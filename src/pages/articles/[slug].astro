---
import "../../styles/main.css";
import Navbar from "../../components/Navbar.astro";
import Footer from "../../components/Footer.astro";
import personPlaceholder from "../../assets/images/person-placeholder.jpg";
import { Image } from 'astro:assets';
import { allCompanyInformation } from "../../lib/seoContent"

const seoContent = await allCompanyInformation();

const keywords = seoContent[0]?.seoKeywords?.join(", ") || "";


import { allArticles } from "../../lib/allArticles";

const articles = await allArticles();
export async function getStaticPaths() {
  const articles = await allArticles();
  
  return articles.map((article: { slug: string }) => ({
    params: { slug: article.slug },
    props: { article },
  }));
}

const { slug } = Astro.params;
const { article } = Astro.props;

