---
import "../styles/main.css";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
import acTechnology from "../assets/images/acTechnology.jpg";
import { Image } from 'astro:assets';
import Blogpreview from "../components/Blogpreview.astro";
import ServiceBanner from "../components/ServiceBanner.astro";

import { allCompanyInformation } from "../lib/seoContent"
import PrimaryButton from "../components/Buttons/PrimaryButton.astro";

const seoContent = await allCompanyInformation();

const keywords = seoContent[0]?.seoKeywords?.join(", ") || "";

---
<!DOCTYPE html>

<html  data-wf-page="662c9d3db09babfb8889cce6"
    data-wf-site="661caa5ae3dacb8bb23f4d00" lang="en" data-wf-locale="en">

<head>
    <meta charset="utf-8" />
    <title>Services - {seoContent[0].companyName}</title>
    <meta
        content={seoContent[0].seoDescription}
        name="description" />
    <meta content={`Services - ${seoContent[0].companyEmail}`} property="og:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="og:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="og:image" />
    <meta content={`Services - ${seoContent[0].companyEmail}`} property="twitter:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="twitter:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
   
    <meta name="keywords" content={keywords} />
   
    
    
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>

    <script
        type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="icon" type="image/png" />
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="apple-touch-icon" sizes="180x180" />
        
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-28TP2W8ST9"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-28TP2W8ST9');
</script>
<script id="Cookiebot" src="https://consent.cookiebot.com/uc.js" data-cbid="754e5917-acec-4eaf-8532-75ab22a6a07f" type="text/javascript" async></script>
</head>

<body>
    <Navbar/>
    <section class="about-banner-section service">
        <div class="w-layout-blockcontainer container w-container">
            <div class="about-banner">
                <div data-w-id="10457758-bf2a-dd1d-ed4b-6b3fa9c5060a" style="Opacity:1"
                    class="about-banner-content-wrap">
                    <h1 class="banner-title">Stay Cool with Our Expert AC Solutions</h1>
                    <p class="banner-content">Ensure comfort and efficiency with our reliable air conditioning services, designed to keep your space cool and your systems running smoothly.</p>
                    <PrimaryButton text="Contact Us" href="/contact" />
                </div>
                <div data-w-id="10457758-bf2a-dd1d-ed4b-6b3fa9c50618" style="Opacity:1" class="about-banner-image-wrap">
                    <Image src={acTechnology}
                        loading="eager"
                        alt="service banner" class="about-banner-image" /></div>
            </div>
        </div>
        <div class="w-layout-blockcontainer large-container w-container">
            <div class="hero-counter-wrap">
                <div class="hero-counter">
                    <h2 class="counter-count">100%</h2>
                    <div class="counter-text-wrap">
                        <div class="color-swatch-bg-icon-wrap">
                            <div class="color-swatch-bg-icon"></div>
                        </div>
                        <div>On-Time Deliveries</div>
                    </div>
                </div>
                <div class="hero-counter">
                    <h3 class="counter-count">7254</h3>
                    <div class="counter-text-wrap">
                        <div class="color-swatch-bg-icon-wrap">
                            <div class="color-swatch-bg-icon"></div>
                        </div>
                        <div>Drop-offs</div>
                    </div>
                </div>
                <div class="hero-counter">
                    <h3 class="counter-count">95%</h3>
                    <div class="counter-text-wrap">
                        <div class="color-swatch-bg-icon-wrap">
                            <div class="color-swatch-bg-icon"></div>
                        </div>
                        <div>Cross-Border Services</div>
                    </div>
                </div>
                <div class="hero-counter">
                    <h3 class="counter-count">3520</h3>
                    <div class="counter-text-wrap">
                        <div class="color-swatch-bg-icon-wrap">
                            <div class="color-swatch-bg-icon"></div>
                        </div>
                        <div>Happy Customers</div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <ServiceBanner/>
    <Blogpreview/>
    <Footer/>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=661caa5ae3dacb8bb23f4d00"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="../script/app.js"> </script>
</body>

</html>