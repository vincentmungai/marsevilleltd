---
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
import acTechnology from "../assets/images/acTechnology.jpg";
import acClient from "../assets/images/acClient.webp";
import acUnitImage from "../assets/images/ac02.png";
import "../styles/main.css";
import { Image } from "astro:assets";
import Blogpreview from "../components/Blogpreview.astro";
import ClientLogo from "../components/ClientLogo.astro";

import { allCompanyInformation } from "../lib/seoContent"
import PrimaryButton from "../components/Buttons/PrimaryButton.astro";

const seoContent = await allCompanyInformation();

const keywords = seoContent[0]?.seoKeywords?.join(", ") || "";

---
<!DOCTYPE html>

<html  data-wf-page="661eb7d0d33ad1e604f69743"
    data-wf-site="661caa5ae3dacb8bb23f4d00" lang="en" data-wf-locale="en">

<head>
    <meta charset="utf-8" />
    <title>{`About - ${seoContent[0].companyEmail}`}</title>
    <meta
        content={seoContent[0].seoDescription}
        name="description" />
    <meta content={`About - ${seoContent[0].companyEmail}`} property="og:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="og:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="og:image" />
    <meta content={`About - ${seoContent[0].companyEmail}`} property="twitter:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="twitter:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
   
    <meta name="keywords" content={keywords} />
   
    
    
    

    <script
        type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="icon" type="image/png" />
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="apple-touch-icon" sizes="180x180" />
        
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-28TP2W8ST9"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-28TP2W8ST9');
</script>

</head>

<body>
    <Navbar/>
    <section class="about-banner-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="about-banner">
                <div data-w-id="6266cd12-bbbf-8f95-b9eb-31631f22c29b" style="opacity:1"
                    class="about-banner-content-wrap">
                    <div class="about-banner-title-wrap">
                        <h1 class="banner-title">Leading the Future of Cooling Technology</h1>
                    </div>
                    <p class="banner-content">Transforming the air conditioning industry with innovative solutions and energy-efficient strategies for optimal comfort.</p>
                   <PrimaryButton text="Contact Us" href="/contact" />
                </div>
                <div data-w-id="6266cd12-bbbf-8f95-b9eb-31631f22c2a9" style="opacity:1" class="about-banner-image-wrap">
                    <Image src={acTechnology}
                        loading="eager"
                        alt="about banner " class="about-banner-image" /></div>
            </div>
            <div class="spacer" style="height:32px;"></div>
            <ClientLogo/>
        </div>
    </section>
    <section class="global-heading-section">
        <div class="w-layout-blockcontainer container w-container">
            <div data-w-id="f93835e2-7fba-879f-8356-78b6eb0439ba" style="opacity:1" class="global-title-wrap">
                <h2 class="section-title">AC Solutions Experts</h2>
                <p class="section-content">We are Air conditioning solutions experts who provide seamless solutions to all of your
                    ac needs.</p>
            </div>
        </div>
    </section>
    <section class="our-mission-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="mission-wrap">
                <div data-w-id="dcbe3fd6-f751-ba05-778e-5fdd040ee758" style="opacity:1" class="mission-content-wrap">
                    <div class="mission-top-content">
                        <h2 class="section-title white-text">Our Mission</h2>
                        <p class="section-content white-text">Ensuring comfort and reliability with top-quality air conditioning solutions, enhancing indoor air quality, and providing energy-efficient cooling for homes and businesses.</p>
                    </div>
                    <div class="mission-top-content">
                        <h2 class="section-title white-text">Our Goals</h2>
                        <p class="section-content white-text">Our mission is to deliver efficient and dependable cooling solutions tailored to our customers' unique needs.</p>
                        <div class="about-sub-wrap-list mission-list">
                            <div class="about-list-item">
                                <div class="about-sub-wrap-icon-bg"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/661e6072cb89e63ddaef303f_about-tick-sub-wrap.svg"
                                        loading="lazy" alt="list images" /></div>
                                <div class="about-list-content-sub-wrap">
                                    <p class="about-list-item-content sub-wrap-content">Optimizing Cooling Efficiency.</p>
                                </div>
                            </div>
                            <div class="about-list-item">
                                <div class="about-sub-wrap-icon-bg"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/661e6072cb89e63ddaef303f_about-tick-sub-wrap.svg"
                                        loading="lazy" alt="list images" /></div>
                                <div class="about-list-content-sub-wrap">
                                    <p class="about-list-item-content sub-wrap-content">Ensuring <span
                                            class="about-content-subtext">Timely Installations & Maintenance.</span></p>
                                </div>
                            </div>
                            <div class="about-list-item">
                                <div class="about-sub-wrap-icon-bg"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/661e6072cb89e63ddaef303f_about-tick-sub-wrap.svg"
                                        loading="lazy" alt="list images" /></div>
                                <div class="about-list-content-sub-wrap">
                                    <p class="about-list-item-content sub-wrap-content">Customized Climate Control Solutions.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div data-w-id="0e88b614-22d1-f62b-916c-84ee030c4bbd" class="mission-image-wrap"><Image
                        src={acClient}
                        loading="lazy"
                        alt="about image" class="mission-image" style="max-height: 500px; max-width: 390px; object-fit: cover;"/>
                    <div style="width:100%;height:100%;" class="mission-image-overlay">
                    </div>
                </div>
            </div>
        </div>
    </section>
    
    
    <section class="pricing-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="pricing-wrap">
                <div data-w-id="51754467-b8b3-2b74-8047-59b3de75508a" style="opacity:1" class="pricing-title-wrap">
                    <h2 class="section-title">Top notch services for all our clients</h2>
                    <p class="section-content">Experience reliable and energy-efficient air conditioning solutions at competitive prices, delivering exceptional value and comfort for your space.</p>
                    <PrimaryButton text="Contact Us" href="/contact" />
                </div>
                <div class="pricing-card-whole-wrap">
                    <div data-w-id="416de2b8-5392-0151-68bc-7ca3f999dac1" style="opacity:1" class="pricing-card">
                        <!-- <div class="pricing-top-rate-wrap">
                            <h3 class="pricing-name"></h3>
                            
                        </div> -->
                        <div class="pricing-list-wrap">
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content">Standard AC Installation</p>
                            </div>
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content">Quick Repair Services</p>
                            </div>
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content">24/7 Customer Support</p>
                            </div>
                            
                        </div>
                       
                    </div>
                    <div data-w-id="ccd05ea5-b274-f5f4-e380-acdc9b86c952" style="opacity:1"
                        class="pricing-card premium">
                        <div class="pricing-top-rate-wrap">
                            <h3 class="pricing-name premium">Exclusive services</h3>
                            <!-- <h2 class="pricing-rate premium"><span class="price-symbol-subtext">$</span><span
                                    class="pricing-rate-subtext">250</span> Per Month</h2> -->
                        </div>
                        <div class="pricing-list-wrap">
                           
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content premium">Advanced Diagnostics</p>
                            </div>
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content premium">Priority Service</p>
                            </div>
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content premium">Exclusive Maintenance Deals</p>
                            </div>
                            <div class="pricing-list">
                                <div class="pricing-list-icon-wrap"><img
                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662b9bad2615b3e3a870925d_pricing-list.svg"
                                        loading="lazy" alt="price list" class="pricing-list-icon" /></div>
                                <p class="pricing-list-content premium">Smart AC System Integration</p>
                            </div>
                        </div>
                        <!-- <div class="pricing-card-button-wrap"><a data-w-id="4c12e140-4e3b-3fd4-4538-c2a3f13c3c62"
                                href="/blog" class="main-button-link premium-button w-inline-block">
                                <div>Purchase Now</div>
                                <div class="button-icon-wrap">
                                    <div style="-webkit-transform:translate3d(3px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);-moz-transform:translate3d(3px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);-ms-transform:translate3d(3px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);transform:translate3d(3px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0)"
                                        class="button-icon icon-one"></div>
                                    <div style="-webkit-transform:translate3d(5px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);-moz-transform:translate3d(5px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);-ms-transform:translate3d(5px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0);transform:translate3d(5px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(-45deg, 0)"
                                        class="button-icon icon-two"></div>
                                    <div style="background-color:rgb(0,0,0)" class="button-icon"></div>
                                </div>
                            </a></div> -->
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="feedback-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="feedback-wrap">
                <div class="feedback-image-wrap"><Image class="feedback-image"
                        src={acUnitImage}
                        alt="cta bg" style="opacity:1" sizes="100vw" data-w-id="358b97a4-59b6-10e4-55d7-f20ab84e3148"
                        loading="lazy" />
                </div>
                <div class="spacer" style="width: 128px;"></div>
                <div data-w-id="3b814f7e-98d3-1ec4-173f-a13330845a95" style="opacity:1" class="feedback-cta-card">
                    <h2 class="section-title white-text">Require Assistance? Grab Your Free Consultation!</h2>
                    <div class="feedback-link-wrap">
                        <div class="feedback-icon-wrap"><img
                                src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bda2442cc9a329b2c5037_phone.svg"
                                loading="lazy" alt="cta phone icon" /></div>
                        <div class="feedback-text-wrap">
                            <p class="feedback-cta-content">Got any queries?</p><a href={`tel:${seoContent[0].companyPhone}`}
                                class="feedback-card-link">+254702804812</a>
                        </div>
                    </div>
                    <div class="feedback-cta-button-wrap"><a href="contact" class="secondary-button w-button" style="color: white !important;">Call Us</a></div>
                </div>
            </div>
        </div>
    </section>
    <section class="faq-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="faq-wrap">
                <div data-w-id="61c8800a-5d2e-afec-780c-397b0cd33737" class="faq-title-wrap">
                    <h2 class="section-title">Frequently Asked Questions</h2>
                    <p class="section-content">Explore answers to common inquiries about our Air Conditioning services here.
                    </p>
                </div>
                <div data-w-id="61c8800a-5d2e-afec-780c-397b0cd3373c" class="faq-main-wrap">
                    <div data-hover="false" data-delay="0" data-w-id="61c8800a-5d2e-afec-780c-397b0cd3373d"
                        class="faq w-dropdown">
                        <div class="faq-toggle top w-dropdown-toggle">
                            <div>What areas do you serve?</div>
                            <div class="faq-icon-wrap"><img
                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bee41b1ecfbffc0c320f9_faq-arrrow.svg"
                                    loading="lazy" alt="faq arrow" class="faq-icon" /></div>
                        </div>
                        <nav class="faq-list w-dropdown-list">
                            <div class="faq-answer-wrap">
                                <p class="faq-content">We provide air conditioning installation, maintenance, and repair services across Nairobi. Contact us to check availability in your location.</p>
                            </div>
                        </nav>
                    </div>
                    <div data-hover="false" data-delay="0" data-w-id="61c8800a-5d2e-afec-780c-397b0cd33747"
                        class="faq w-dropdown">
                        <div class="faq-toggle w-dropdown-toggle">
                            <div>How do I schedule a service?</div>
                            <div class="faq-icon-wrap"><img
                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bee41b1ecfbffc0c320f9_faq-arrrow.svg"
                                    loading="lazy" alt="faq arrow" class="faq-icon" /></div>
                        </div>
                        <nav class="faq-list w-dropdown-list">
                            <div class="faq-answer-wrap">
                                <p class="faq-content">Booking is easy! You can schedule an appointment through our website, call our customer service team, or send us a message via email.</p>
                            </div>
                        </nav>
                    </div>
                    <div data-hover="false" data-delay="0" data-w-id="61c8800a-5d2e-afec-780c-397b0cd33751"
                        class="faq w-dropdown">
                        <div class="faq-toggle w-dropdown-toggle">
                            <div>Are there size limitations for AC units?</div>
                            <div class="faq-icon-wrap"><img
                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bee41b1ecfbffc0c320f9_faq-arrrow.svg"
                                    loading="lazy" alt="faq arrow" class="faq-icon" /></div>
                        </div>
                        <nav class="faq-list w-dropdown-list">
                            <div class="faq-answer-wrap">
                                <p class="faq-content">We service and install all types of AC units, from small residential systems to large commercial cooling solutions. If you're unsure, our experts can recommend the best option for your space.</p>
                            </div>
                        </nav>
                    </div>
                    <div data-hover="false" data-delay="0" data-w-id="61c8800a-5d2e-afec-780c-397b0cd3375b"
                        class="faq w-dropdown">
                        <div class="faq-toggle w-dropdown-toggle">
                            <div>What is your cancellation policy?</div>
                            <div class="faq-icon-wrap"><img
                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bee41b1ecfbffc0c320f9_faq-arrrow.svg"
                                    loading="lazy" alt="faq arrow" class="faq-icon" /></div>
                        </div>
                        <nav class="faq-list w-dropdown-list">
                            <div class="faq-answer-wrap">
                                <p class="faq-content">We offer flexible cancellations with at least [X] hours' notice. Late cancellations may be subject to a small fee. Contact us for details.</p>
                            </div>
                        </nav>
                    </div>
                    <div data-hover="false" data-delay="0" data-w-id="61c8800a-5d2e-afec-780c-397b0cd33765"
                        class="faq w-dropdown">
                        <div class="faq-toggle w-dropdown-toggle">
                            <div>Do you offer warranty or insurance options?</div>
                            <div class="faq-icon-wrap"><img
                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/662bee41b1ecfbffc0c320f9_faq-arrrow.svg"
                                    loading="lazy" alt="faq arrow" class="faq-icon" /></div>
                        </div>
                        <nav class="faq-list w-dropdown-list">
                            <div class="faq-answer-wrap">
                                <p class="faq-content">Yes! We provide warranty coverage on installations and repairs. Additionally, we offer maintenance plans to keep your AC running efficiently year-round.</p>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- <Blogpreview/> -->
    <Footer/>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=661caa5ae3dacb8bb23f4d00"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></>
    <script src="../script/app.js"> </script>
</body>

</html>