---
import "../styles/main.css";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
import { Image } from 'astro:assets';
import { allArticles } from "../lib/allArticles";

const articles = await allArticles();


import { allCompanyInformation } from "../lib/seoContent"

const seoContent = await allCompanyInformation();

const keywords = seoContent[0]?.seoKeywords?.join(", ") || "";

---
<!DOCTYPE html>

<html  data-wf-page="662cba9aa85c97710101d676"
    data-wf-site="661caa5ae3dacb8bb23f4d00" lang="en" data-wf-locale="en">

<head>
    <meta charset="utf-8" />
    <title>Blog - {seoContent[0].companyName}</title>
    <meta
        content={seoContent[0].seoDescription}
        name="description" />
    <meta content={`Blog - ${seoContent[0].companyEmail}`} property="og:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="og:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="og:image" />
    <meta content={`Blog - ${seoContent[0].companyEmail}`} property="twitter:title" />
    <meta
        content={seoContent[0].seoDescription}
        property="twitter:description" />
    <meta
        content={seoContent[0].companyBanner}
        property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
   
    <meta name="keywords" content={keywords} />
    <link
        href="styles/main.css"
        rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com" rel="preconnect" />
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin="anonymous" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script
        type="text/javascript">WebFont.load({ google: { families: ["Catamaran:300,regular,500,600,700,800", "Sarabun:300,regular,500,600,700,800"] } });</script>
    <script
        type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="icon" type="image/png" />
        <link href="https://uhfawvmxqtznjbwhdkyx.supabase.co/storage/v1/object/public/content//brandLogo.png" rel="apple-touch-icon" sizes="180x180" />
        
</head>

<body>
    <Navbar />
    <section class="banner-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="banner-wrap">
                <div data-w-id="f075c094-490f-484e-2933-4115c992e97f" style="opacity:0" class="banner-content-area">
                    <h1 class="banner-title">Top Articles</h1>
                    <p class="banner-content">Gain new perspectives and useful insights from our latest blog post, which
                        provides the most recent updates in the AC business.</p>
                </div>
            </div>
        </div>
    </section>
    <section class="blog-feature-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="w-dyn-list">
                <div role="list" class="w-dyn-items">
                    <div role="listitem" class="w-dyn-item">
                        <div data-w-id="2919d7fe-ef03-2542-7880-ee95f8190174" style="opacity:0"
                            class="blog-feature-wrap">
                            <div class="blog-feature-image-area"><a
                                    href={`articles/${articles[0].slug}`}
                                    class="blog-feature-image-wrap w-inline-block"><img
                                        src={articles[0].coverImage}
                                        loading="eager" alt="blog feature"
                                        class="blog-feature-image" style="height: 100% !important; width: 100% !important; object-fit: center;"/></a>
                                <div class="blog-publish-text">
                                    <div>{articles[0].date}</div>
                                </div>
                            </div>
                            <div class="blog-feature-content-wrap">
                                <div id="w-node-_2919d7fe-ef03-2542-7880-ee95f8190178-0101d676"
                                    class="blog-list-item-wrap feature"><a
                                        href={`articles/${articles[0].slug}`}
                                        class="blog-feature-title">{articles[0].title}</a>
                                    <div class="blog-post-intro-wrap">
                                        <p class="blog-post-intro">{articles[0].excerpt}</p>
                                        <div class="blog-post-read-more-wrap left-align">
                                            <div class="blog-post-read-more-link"><a
                                                    href={`articles/${articles[0].slug}`}
                                                    class="read-more-link feature">Read More About This Blog</a></div>
                                            <div class="read-more-line-break"><a
                                                    style="background-color:rgb(234,234,234)"
                                                    href={`articles/${articles[0].slug}`}
                                                    class="blog-arrow-wrap w-inline-block"><img
                                                        src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/661e4792f078565646926ea4_service-card-arrow.svg"
                                                        loading="lazy" alt="arrow image"
                                                        class="service-card-arrow-icon" /></a></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="blog-post-section">
        <div class="w-layout-blockcontainer container w-container">
            <div class="blog-collection-wrap">
                <div class="w-dyn-list">
                    <div role="list" class="w-dyn-items w-row">
                        
                        {articles.map((article: any)=>(
                        <div role="listitem" class="blog-item w-dyn-item w-col w-col-6">
                            <div id="w-node-_54304a89-817e-8575-235e-36e09b76bdbf-0101d676"
                                data-w-id="54304a89-817e-8575-235e-36e09b76bdbf" style="opacity:0"
                                class="blog-list-item-wrap"><a href={`articles/${article.slug}`}
                                    class="blog-title-link">{article.title}</a>
                                <div class="blog-image-wrap"><a href={`articles/${article.slug}`}
                                        class="blog-image-link w-inline-block"><img
                                            src={article.coverImage}
                                            loading="eager" alt="blog image"
                                            class="blog-image" /></a>
                                    <div class="blog-publish-text">
                                        <div>{article.date}</div>
                                    </div>
                                </div>
                                <div class="blog-post-intro-wrap">
                                    <p class="blog-post-intro">{article.excerpt}</p>
                                    <div class="blog-post-read-more-wrap left-align">
                                        <div class="blog-post-read-more-link"><a
                                                href={`articles/${article.slug}`}
                                                class="read-more-link">Read More About This Blog</a></div>
                                        <div class="read-more-line-break"><a
                                                href={`articles/${article.slug}`}
                                                class="blog-arrow-wrap w-inline-block"><img
                                                    src="https://cdn.prod.website-files.com/661caa5ae3dacb8bb23f4d00/661e4792f078565646926ea4_service-card-arrow.svg"
                                                    loading="lazy" alt="arrow image"
                                                    class="service-card-arrow-icon" /></a></div>
                                    </div>
                                </div>
                            </div>
                        </div> 
                        ))}
                        
                        

                               
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <Footer/>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=661caa5ae3dacb8bb23f4d00"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="../script/app.js"> </script> 
</body>

</html>